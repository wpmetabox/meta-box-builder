{
  "license": "GPL-2.0-or-later",
  "scripts": {
    "css:main": "node-sass --sourcemap none --output-style compressed assets/sass/style.scss -o assets/css",
    "watch:css:main": "node-sass --sourcemap none --output-style compressed -w assets/sass -r -o assets/css",
    "css:settings": "node-sass --sourcemap none --include-path assets/sass --output-style compressed modules/settings-page/assets/settings-page.scss -o modules/settings-page/assets",
    "watch:css:settings": "node-sass --sourcemap none --include-path assets/sass --output-style compressed -w modules/settings-page/assets -r -o modules/settings-page/assets",
    "css:relationships": "node-sass --sourcemap none --include-path assets/sass --output-style compressed modules/relationships/assets/relationships.scss -o modules/relationships/assets",
    "watch:css:relationships": "node-sass --sourcemap none --include-path assets/sass --output-style compressed -w modules/relationships/assets -r -o modules/relationships/assets",
    "build": "webpack --mode production",
    "start": "webpack --mode development",
    "watch": "webpack --mode development --watch",
    "sync": "browser-sync start --proxy \"metabox.test/wp-admin/post-new.php?post_type=meta-box\" --files \"assets/css/style.css, assets/js/app.js\""
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/plugin-transform-react-jsx": "^7.14.9",
    "babel-loader": "^8.2.2",
    "node-sass": "^6.0.1",
    "webpack": "^4.46.0",
    "webpack-cli": "^3.3.12"
  },
  "dependencies": {
    "dot-prop": "^6.0.1",
    "react-codemirror2": "^7.2.1",
    "react-select": "^3.2.0",
    "react-sortablejs": "^6.0.0",
    "react-tabs": "^3.2.2",
    "slugify": "^1.6.1",
    "sortablejs": "^1.14.0"
  }
}
